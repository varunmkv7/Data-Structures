#include<stdio.h>
int main()
{
	int a[10],i,item,n;
	printf("\n enter the number of array:");
	scanf("%d",&n);
	printf("\n enter the element:");
	for(i=0;i<n;i++)
		scanf("%d",&a[i]);
	printf("\n enter item to search:");
	scanf("%d",&item);
	for(i=0;i<=9;i++)
		if(item==a[i])
		{
			printf("\n item found at  %d:",i+1);
			break;
		}
		if(i>9)
			printf("\n item is not found");
			return 0;
}
